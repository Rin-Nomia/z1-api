# =========================
# Continuum Oracle Free env
# =========================

# Required security keys
LOG_SALT=replace-with-32-plus-random-chars
LICENSE_KEY=replace-with-license-decrypt-key
USAGE_SIGNING_KEY=replace-with-sign-key-or-empty

# C3 admin auth (choose hash preferred)
C3_ADMIN_PASSWORD_HASH=pbkdf2_sha256$260000$<salt_b64>$<digest_hex>
# C3_ADMIN_PASSWORD=

# Gateway basic auth
C3_BASIC_AUTH_USER=admin
C3_BASIC_AUTH_HASH=$2a$14$replace-with-caddy-hash

# Public domain (DNS A record -> Oracle VM public IP)
API_HOSTNAME=api.example.com
C3_HOSTNAME=c3.example.com
ACME_EMAIL=you@example.com

# Optional behavior
LICENSE_ENFORCEMENT_MODE=degrade
C3_LOGIN_MAX_ATTEMPTS=5
C3_LOCKOUT_SECONDS=900
C3_SESSION_TTL_SECONDS=1800
Z1_MVP_REF=main
